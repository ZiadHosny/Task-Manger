{
    "version": 2,
    "name": "task-manger-backend",
    "builds": [
        {
            "src": "./backend/dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "backend/dist/**"
                ]
            }
        },
        {
            "src": "./frontend/build/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ]
}